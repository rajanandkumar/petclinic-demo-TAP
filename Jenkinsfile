pipeline{
agent any
stages{
    stage ('Build') {
      steps{
        echo "Building Project"
        
      }
    }
    stage ('Archive') {
      steps{
        echo "Archiving Project"
       
      }
    }
     stage ('Build Docker Image') {
      steps{
        echo "Building Docker Image"
       
      }
    }
     stage ('Push Docker Image') {
      steps{
        echo "Docker Image Pushing"
       
      }
    }
    stage ('Deploy to Dev') {
      steps{
        echo "Deploy to Dev Environment"
       
      }
    }
  }

}
